<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="411dp"
        android:layout_height="1092dp"
        android:background="#000000"
        android:rotation="-360"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.819"
        app:srcCompat="@drawable/baggrund_locobar"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="333dp"
        android:layout_height="135dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="90dp"
        android:fontFamily="@font/griffy"
        android:gravity="center"
        android:text="VELKOMMEN! LOCO BAR"
        android:textColor="#121111"
        android:textSize="48sp"
        android:textStyle="bold"
        app:elevation="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.155" />

    <Button
        android:id="@+id/button"
        android:layout_width="194dp"
        android:layout_height="85dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="332dp"
        android:background="#E2FBFF"
        android:fontFamily="@font/griffy"
        android:onClick="button1"
        android:text="START DAGEN"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#BD4040" />

    <Button
        android:id="@+id/button2"
        android:layout_width="192dp"
        android:layout_height="81dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="468dp"

        android:background="#E2FBFF"

        android:fontFamily="@font/griffy"
        android:onClick="button2"
        android:text="REDIGERE PÃ… MENU"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>